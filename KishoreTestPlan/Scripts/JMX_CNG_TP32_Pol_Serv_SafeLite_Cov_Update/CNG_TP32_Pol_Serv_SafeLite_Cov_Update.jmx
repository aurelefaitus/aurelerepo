		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
	}
}

else
{
	//response is not 200
	log.error(&quot;TP32_010_Pol_Serv_SafeLite_Cov_Update failed for Request XML:&quot;+prev.getSamplerData()+&quot;####&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;####&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
