                <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
                  <boolProp name="resetInterpreter">false</boolProp>
                  <stringProp name="parameters"></stringProp>
                  <stringProp name="filename"></stringProp>
                  <stringProp name="script">int rescode=Integer.parseInt(vars.get(&quot;responsecode&quot;));
String stats= vars.get(&quot;XML_ResponseStatus&quot;);
if (rescode &gt;= 200 &amp;&amp; rescode &lt; 400)
{	//error if no results found
	if (!stats.equals(&quot;Success&quot;))
	{	
		//  search did not return any results
		log.error(&quot;Transaction Name :TP22_010_Agero_Policy_Search##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
		
	}
	else 
	{
		// search successfull
		prev.setSuccessful(true);
	}
}
else
{
// Unexpected result  from server
	log.error(&quot;Transaction Name :TP22_010_Agero_Policy_Search##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
	prev.setSuccessful(false);
	ctx.setRestartNextLoop(true);
}
</stringProp>
