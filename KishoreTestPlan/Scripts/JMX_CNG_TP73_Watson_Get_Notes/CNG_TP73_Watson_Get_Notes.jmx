            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">int rescode=Integer.parseInt(vars.get(&quot;responsecode&quot;));
String Stat1=vars.get(&quot;RESULTS_Count&quot;);
String Stat2=vars.get(&quot;EXCEPTION_Count&quot;);
String Stat3=vars.get(&quot;ERROR_Count&quot;);
String Stat4=vars.get(&quot;INVALID_Count&quot;);
String Stat5=vars.get(&quot;TYPENOTES_Count&quot;);
String Stat6=vars.get(&quot;NONOTES_Count&quot;);
String Stat7=vars.get(&quot;NOTFOUND_Count&quot;);

if (rescode &gt;= 200 &amp;&amp; rescode &lt; 400)
	{	
	if (Stat2.equals(&quot;null&quot;))
		{
			if (Stat3.equals(&quot;null&quot;) &amp;&amp; !Stat1.equals(&quot;null&quot;))
			{
				log.info(&quot;HTTP Response 200 with Note(s) returned for Claim Number: &quot;+vars.get(&quot;LM_PARENTEXPOSURENUMBER&quot;)+&quot; and Level: &quot;+vars.get(&quot;LEVEL&quot;));
				prev.setSuccessful(true);
			}
			if (Stat1.equals(&quot;null&quot;) &amp;&amp; !Stat5.equals(&quot;null&quot;))
			{
				log.info(&quot;HTTP Response 200 but no notes retruned for Claim Number: &quot;+vars.get(&quot;LM_PARENTEXPOSURENUMBER&quot;)+&quot; and Level: &quot;+vars.get(&quot;LEVEL&quot;));
				prev.setSuccessful(true);
			}
			if (!Stat3.equals(&quot;null&quot;) &amp;&amp; !Stat7.equals(&quot;null&quot;))
			{
				log.info(&quot;HTTP Response 200 but Claim Number: &quot;+vars.get(&quot;LM_PARENTEXPOSURENUMBER&quot;)+&quot; was not found&quot;);
				prev.setSuccessful(true);
			}
			if (!Stat3.equals(&quot;null&quot;) &amp;&amp; !Stat6.equals(&quot;null&quot;))
			{
				log.info(&quot;HTTP Response 200 but no occurences for the Claim Number: &quot;+vars.get(&quot;PARENTEXPOSURENUM&quot;));
				prev.setSuccessful(true);
			}
			if (Stat1.equals(&quot;null&quot;) &amp;&amp; !Stat4.equals(&quot;null&quot;))
			{
			log.error(&quot;TP73_010_Watson_Get_Notes failed for Request XML:&quot;+prev.getSamplerData()+&quot;####&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;####&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
			prev.setSuccessful(false);
			ctx.setRestartNextLoop(true);
			}
		}
	else 
		{
		log.error(&quot;TP73_010_Watson_Get_Notes failed for Request XML:&quot;+prev.getSamplerData()+&quot;####&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;####&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
		}
	}

else
	{
	log.error(&quot;TP73_010_Watson_Get_Notes failed for Request XML:&quot;+prev.getSamplerData()+&quot;####&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;####&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
	prev.setSuccessful(false);
	ctx.setRestartNextLoop(true);
	}


</stringProp>
