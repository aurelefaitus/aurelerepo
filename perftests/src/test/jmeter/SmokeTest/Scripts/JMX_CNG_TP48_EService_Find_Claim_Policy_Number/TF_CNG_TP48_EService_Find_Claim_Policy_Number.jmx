              <stringProp name="script">int rescode=Integer.parseInt(vars.get(&quot;responsecode&quot;));
String stats= vars.get(&quot;validation_1&quot;);
if (rescode &gt;= 200 &amp;&amp; rescode &lt; 400)
{	//error if no results found
	if (!stats.equals(&quot;null&quot;))
	{	
		//  search did not return any results
		log.error(&quot;Transaction Name :CNG_TP03_C_Activities_Click_Search##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
		
	}
	else 
	{
		// search successfull
		prev.setSuccessful(true);
	}
}
else
{
// Unexpected result  from server
	log.error(&quot;Transaction Name :CNG_TP03_C_Activities_Click_Search##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
	prev.setSuccessful(false);
	ctx.setRestartNextLoop(true);
}
</stringProp>
