if (rescode &gt;= 200 &amp;&amp; rescode &lt; 400)
{	//error if no results found
	if (!stats.equals(&quot;null&quot;) &amp;&amp; stats1.equals(&quot;null&quot;))
	{	
		//  search did not return any results
		log.error(&quot;Transaction Name :TP99_010_eService_Get_Express_Review##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
		
	}
	else if (!stats.equals(&quot;null&quot;) &amp;&amp; !stats1.equals(&quot;null&quot;))
	{
		// search successfull
		prev.setSuccessful(true);
	}
	else
	{
		log.error(&quot;Transaction Name :TP99_010_eService_Get_Express_Review##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
		prev.setSuccessful(false);
		ctx.setRestartNextLoop(true);
	}
}
else
{
// Unexpected result  from server
	log.error(&quot;Transaction Name :TP99_010_eService_Get_Express_Review##&quot;+&quot;Request XML:&quot;+prev.getSamplerData()+&quot;##&quot;+&quot;HTTP Response Code:&quot;+vars.get(&quot;responsecode&quot;)+&quot;##&quot;+&quot;Response From Server : &quot;+prev.getResponseDataAsString());
	prev.setSuccessful(false);
	ctx.setRestartNextLoop(true);
}
